fun counter(a) {
   let x = a;
   fun c() {
      let x = x + 1;
      return x;
   }
   return c;
};

let incrementOne = counter(1);

println("Demonstrating closure state, by calling incrementOne() returned from counter consecutively");
println("");

println(incrementOne());
println(incrementOne());
println(incrementOne());
println(incrementOne());

println("");
println("OMG THIS IS SO FRIGGING COOL");
println("");
